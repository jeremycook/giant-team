{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "ES2020",
    "moduleResolution": "NodeNext",
    "outDir": "wwwroot/js",
    "rootDir": "TS"
  },
  "include": [ "TS" ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
