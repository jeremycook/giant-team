{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "ApiRoutes": {
        "ClusterId": "Api",
        "Match": {
          "Path": "/api/{**remainder}"
        }
      },
      "SwaggerRoutes": {
        "ClusterId": "Swagger",
        "Match": {
          "Path": "/swagger/{**remainder}"
        }
      },
      "UIRoutes": {
        "ClusterId": "UI",
        "Match": {
          "Path": "{**catchall}"
        }
      }
    },
    "Clusters": {
      "Api": {
        "Destinations": {
          "WebApp": {
            "Address": "https://localhost:7077"
          }
        }
      },
      "Swagger": {
        "Destinations": {
          "WebApp": {
            "Address": "https://localhost:7077"
          }
        }
      },
      "UI": {
        "Destinations": {
          "SolidUI": {
            "Address": "http://localhost:3000"
          }
        }
      }
    }
  }
}
